<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta
      http-equiv="X-UA-Compatible"
      content="IE=edge"
    />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0"
    />
    <title>Tabela</title>
  </head>
  <body>
    <table>
      <!-- cabecalho da tabela -->
      <thead>
        <% header.forEach(col => { %>
        <!-- colunas  -->
        <th><%= col %></th>
        <% }) %>
      </thead>
      <tbody>
        <!-- corpo da tabela -->
        <!-- percorre pela linha da tabela  -->
        <% rows.forEach(row => { %>
        <!-- linhas -->
        <tr>
          <!-- percorre entro da linha printando os dados -->
          <% row.forEach(element => { %>
          <td><%= element %></td>
          <% }) %>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
